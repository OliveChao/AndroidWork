<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.newthing"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.newthing.MainPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.newthing.AddPage"
            android:label="@string/title_activity_add_page" >
        </activity>
        <activity
            android:name="com.example.newthing.SubtractPage"
            android:label="@string/title_activity_subtract_page" >
        </activity>
        <activity
            android:name="com.example.newthing.LoginPage"
            android:label="@string/title_activity_login_page" >
        </activity>
        <activity
            android:name="com.example.newthing.CounterPage"
            android:label="@string/title_activity_counter_page" >
        </activity>
        <activity
            android:name="com.example.newthing.NotificationPage"
            android:label="@string/title_activity_notification_page" >
        </activity>
        <activity
            android:name="com.example.newthing.SpinnerPage"
            android:label="@string/title_activity_spinner_page" >
        </activity>
    </application>

</manifest>
